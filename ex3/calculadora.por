programa {
  funcao inicio() {
    real num1, num2
    escreva("Digite o primeiro numero: ")
    leia(num1)
    escreva("Digite o segundo numero: ")
    leia(num2)
    escreva("A Soma é igual a: ", num1 + num2, "\n")
    escreva("A Subtração é igual a: ", num1 - num2, "\n")
    escreva("A Multiplicação é igual a: ", num1 * num2, "\n")
    escreva("A Divisão é igual a: ", num1 / num2)
  }
}
